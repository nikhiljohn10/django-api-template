#!/bin/bash

ROOT_DIR=$(pwd)
APP_DIR=$ROOT_DIR/app
rm -rf $APP_DIR/db.sqlite3 $APP_DIR/ssl $APP_DIR/.env .$APP_DIR/env.back $APP_DIR/venv
find $APP_DIR/api/migrations -type f ! -name __init__.py -exec rm -rf {} +
find $ROOT_DIR -name __pycache__ -exec rm -rf {} +
find $ROOT_DIR -name "*.log" -exec rm -rf {} +
find $ROOT_DIR -name ".DS_Store" -exec rm -rf {} +
find $ROOT_DIR -name "Thumb.db" -exec rm -rf {} +
